<UserControl x:Class="MeisterGeister.View.Reise.WetterGraph"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vm="clr-namespace:MeisterGeister.ViewModel.Reise"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">
    <UserControl.Resources>
        <vm:WetterToPathConverter x:Key="converter"/>
    </UserControl.Resources>
    <Path Stroke="Black" StrokeThickness="1" Data="{Binding Converter={StaticResource converter}}">
    </Path>
</UserControl>
