                                            Height="25"
                                            Margin="0,10,2,0"
                                            HorizontalAlignment="Right"
                                            Source="/Images/Icons/General/y-offset.png" />
                                        <Image
                                            Grid.Row="0"
                                            Grid.RowSpan="2"
                                            Height="25"
                                            Margin="20,20,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Top"
                                            Source="/Images/Icons/General/x-offset.png" />

                                        <StackPanel
                                            Margin="24,0,-4,-5"
                                            VerticalAlignment="Top"
                                            Orientation="Horizontal">
                                            <Image
                                                Height="15"
                                                HorizontalAlignment="Left"
                                                Source="/Images/Icons/General/size.png" />
                                            <Slider
                                                Width="45"
                                                Margin="0,2"
                                                VerticalAlignment="Top"
                                                LargeChange="10"
                                                Maximum="20000"
                                                Minimum="0"
                                                MouseWheel="sl_MouseWheel"
                                                Orientation="Horizontal"
                                                SmallChange="1"
                                                Style="{StaticResource SmallHorizontalSlider}"
                                                ToolTip="Größe -  Mausrad drehen um Feinjustierung durchzuführen"
