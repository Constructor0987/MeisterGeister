                        Margin="2,2,0,0"
                        HorizontalAlignment="Left"
                        BorderBrush="Silver"
                        BorderThickness="1">
                        <StackPanel Margin="0,-5,0,0" Orientation="Horizontal">
                            <Grid Width="105">
                                <GroupBox
                                    Grid.RowSpan="3"
