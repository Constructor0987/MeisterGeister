                                            ToolTip="Y-Offset -  Mausrad drehen um Feinjustierung durchzuführen"
                                            Value="{Binding InvBackgroundOffsetY}">
                                            <Slider.RenderTransform>
                                                <TransformGroup>
                                                    <ScaleTransform ScaleX="-1" />
                                                    <SkewTransform />
                                                    <RotateTransform />
                                                    <TranslateTransform />
                                                </TransformGroup>
                                            </Slider.RenderTransform>
