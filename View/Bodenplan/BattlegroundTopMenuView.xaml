                            <Button
                                Grid.Row="2"
                                Grid.ColumnSpan="2"
                                Width="120"
                                Height="20"
                                Margin="0,0"
                                Padding="0"
                                HorizontalAlignment="Center"
                                Command="{Binding OnBtnCenterMeisterView}"
                                Content="Meister Screen Center"
                                ToolTip="Auf Spieler zentrieren" />

                            <ToggleButton
                                Grid.Row="3"
                                Width="22"
                                Height="22"
                                Margin="10,3,10,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                IsChecked="{Binding RechteckGrid}"
                                ToolTip="Rechteck Raster benutzen">
                                <ToggleButton.Content>
                                    <Image
                                        Margin="-2"
                                        Source="/DSA MeisterGeister;component/Images/Icons/rechteckig.png"
                                        Stretch="Fill" />
                                </ToggleButton.Content>
                            </ToggleButton>
                            <ToggleButton
                                Grid.Row="3"
                                Width="22"
                                Height="22"
                                Margin="35,3,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                IsChecked="{Binding HexGrid}"
                                ToolTip="Hexfelder benutzen">
                                <ToggleButton.Content>
                                    <Image
                                        Margin="-2"
                                        Source="/DSA MeisterGeister;component/Images/Icons/hex.png"
                                        Stretch="Fill" />
                                </ToggleButton.Content>
                            </ToggleButton>
                            <xctk:ColorPicker
                                Grid.Row="3"
                                Grid.Column="1"
                                Width="22"
                                Height="22"
                                Margin="0,3"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                SelectedColor="{Binding GridColor, UpdateSourceTrigger=PropertyChanged}"
                                ShowDropDownButton="False"
                                ToolTip="Rasterfarbe" />
                        </Grid>
                    </Border>
