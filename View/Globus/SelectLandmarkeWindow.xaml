        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Landmarke auswählen" Height="439" Width="682" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner" Background="{StaticResource Background1}">
    <Grid Name="_grid">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Button Content="OK"  Grid.Row="1" Margin="12" Name="_buttonLadeDG" Click="_buttonOK" HorizontalAlignment="Right" Width="43" Height="23" ToolTip="Landmarke auswählen." />
    </Grid>
</Window>
